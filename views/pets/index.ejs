<% layout('layouts/boilerplate') %>
   <div class="row">
       <div class="col-12 mt-auto">
            <% for(let i = 0; i < animals.length; i++) %>
             <% if (animals[0].primary_photo_cropped !== null) { %>
                   <img src="<%= animals[0].primary_photo_cropped.small %>" class="img-fluid img-thumbnail rounded mx-auto d-block" alt="Pet Photo">
               <% } else { %>
                   <img src="https://images.unsplash.com/photo-1623387641168-d9803ddd3f35?ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&ixlib=rb-1.2.1&auto=format&fit=crop&w=300&q=67" class="img-fluid img-thumbnail rounded mx-auto d-block" alt="Cat & Dog Photo">
               <% } %>
       
       </div>
   </div> 


